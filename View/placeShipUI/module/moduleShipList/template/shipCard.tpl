<div class="row shipCard panel panel-default " >
<div class="shipName">
    {{shipName}}
     <img class="shipInfo pull-right" src="../placeShipUI/img/Info.png">
</div>
<div class="shipImgContainer">
  <img class="shipImg" src="{{shipImg}}" data-dim="{{shipDim}}">
</div>
<div class="shipWeightContainer" data-weight="{{shipWeight}}">
   <img  class="shipWeightImg" src="{{shipWeightImg}}">
   <p class="shipWeightText">{{shipWeight}}</p>
</div>
</div>